config_version=5

[application]

config/name="TopDownShooter"
config/description="A mini-game created to explore the Godot engine."
config/version="1.0.0"
run/main_scene="res://scenes/levels/level.tscn"
config/features=PackedStringArray("4.3", "Forward Plus")
boot_splash/image="res://src/screensaver/untitled1.png"
config/icon="res://src/icon/bullet-bill.svg"

[autoload]

YandexSDK="*res://addons/yandex_sdk/yandex_sdk.gd"

[display]

window/size/viewport_width=1280
window/size/viewport_height=720
window/stretch/mode="viewport"

[editor_plugins]

enabled=PackedStringArray("res://addons/yandex_sdk/plugin.cfg")

[file_customization]

folder_colors={
"res://addons/": "purple",
"res://license/": "red",
"res://scenes/": "orange",
"res://scripts/": "yellow",
"res://src/": "blue"
}

